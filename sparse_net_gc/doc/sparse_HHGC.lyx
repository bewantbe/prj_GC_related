\begin_layout Standard
Define
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
S^{(uv)}=\left(b_{jk}\right)=\left(D_{t-j}^{(u)}\mathrm{E}(x_{t-j}^{(u)}x_{t-k}^{(v)T})D_{t-k}^{(v)}\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\vec{s}^{(u|v)}=\left(b_{k}\right)^{T}=\left(D_{t}^{(u)}\mathrm{E}(x_{t}^{(u)}x_{t-k}^{(v)T})D_{t-k}^{(v)}\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
D_{t}^{(u)}=\left[\begin{array}{cccc}
1/\sqrt{\mathrm{var}(x_{t}^{(u[1])})}\\
 & 1/\sqrt{\mathrm{var}(x_{t}^{(u[2])})}\\
 &  & \ddots\\
 &  &  & 1/\sqrt{\mathrm{var}(x_{t}^{(u[n_{u}])})}
\end{array}\right],
\end{equation}

\end_inset

where 
\begin_inset Formula $x_{t}^{(u[j])}$
\end_inset

 means the 
\begin_inset Formula $j$
\end_inset

-th component of 
\begin_inset Formula $x_{t}^{(u)}$
\end_inset

, 
\begin_inset Formula $n_{u}$
\end_inset

 is the dimension of 
\begin_inset Formula $x_{t}^{(u)}$
\end_inset

.
 This definition is consist with the 
\begin_inset Formula $R^{(uv)}$
\end_inset

 after normalize all components of 
\begin_inset Formula $x_{t}^{(u)}$
\end_inset

.
\end_layout

\begin_layout Standard
Define
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\Lambda^{(u)}=\left[\begin{array}{cccc}
D_{t}^{(u)}\\
 & D_{t-1}^{(u)}\\
 &  & \ddots\\
 &  &  & D_{t-m}^{(u)}
\end{array}\right],
\end{equation}

\end_inset

so
\begin_inset Formula 
\begin{equation}
S^{(uv)}=\Lambda^{(u)}R^{(uv)}\Lambda^{(v)},
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\vec{s}^{(u|v)}=D_{t}^{(u)}\vec{v}^{(u|v)}\Lambda^{(v)}.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Accordingly
\begin_inset Formula 
\[
\left[\begin{array}{ccc}
v^{(x)}I & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & v^{(y)}I & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & v^{(z)}I
\end{array}\right]^{-1}=\left[\begin{array}{ccc}
\Lambda^{(x)}\\
 & \Lambda^{(y)}\\
 &  & \Lambda^{(z)}
\end{array}\right]\left[\begin{array}{ccc}
S^{(xx)} & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & S^{(yy)} & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & S^{(zz)}
\end{array}\right]^{-1}\left[\begin{array}{ccc}
\Lambda^{(x)}\\
 & \Lambda^{(y)}\\
 &  & \Lambda^{(z)}
\end{array}\right]
\]

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\begin{eqnarray}
 &  & \mathrm{var}(x_{t}^{(1)})-\mathrm{var}(\epsilon_{t}^{(1)})\nonumber \\
 & = & \left[\begin{array}{ccc}
0 & \vec{v}^{(x|y)} & \vec{v}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
v^{(x)}I & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & v^{(y)}I & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & v^{(z)}I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{v}^{(x|y)T}\\
\vec{v}^{(x|z)T}
\end{array}\right]\nonumber \\
 & = & \left(D_{t}^{(x)}\right)^{-1}\left(\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
S^{(xx)} & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & S^{(yy)} & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & S^{(zz)}
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right]\right)\left(D_{t}^{(x)}\right)^{-1}.\label{eq: e1}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $x_{t}$
\end_inset

, 
\begin_inset Formula $y_{t}$
\end_inset

 are scalar, then 
\begin_inset Formula $S^{(xx)}=S^{(yy)}=I_{m\times m}$
\end_inset

.
 And the middle (parenthesized, note as 
\begin_inset Formula $\blacklozenge$
\end_inset

 below) part of Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: e1"

\end_inset

 is also scalar, hence
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathrm{var}(x_{t}^{(1)})-\mathrm{var}(\epsilon_{t}^{(1)})=\left(D_{t}^{(x)}\right)^{-2}\blacklozenge=\mathrm{var}(x_{t}^{(1)})\blacklozenge
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Further, If 
\begin_inset Formula $\mathrm{E}(x_{t}^{(z[j])}x_{t}^{(z[k])})=0,\ \forall j\neq k$
\end_inset

, then 
\begin_inset Formula $S^{(zz)}=I_{(p-2)m\times(p-2)m}$
\end_inset

, which lead to Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: var(eps)<-S"

\end_inset

.
\end_layout

