\begin_layout Subsection
Expression for Three-Variable GC
\end_layout

\begin_layout Standard
Assume 
\begin_inset Formula $\mathrm{E}(x_{t})=\mathrm{E}(y_{t})=\mathrm{E}(y_{t})=0$
\end_inset

, as usual, let's do a 3-var AR of order 
\begin_inset Formula $m$
\end_inset

:
\begin_inset Formula 
\begin{equation}
\left\{ \begin{aligned}x_{t}=\sum_{j=1}^{m}a_{j}^{(11)}x_{t-j}+\sum_{j=1}^{m}a_{j}^{(12)}y_{t-j}+\sum_{j=1}^{m}a_{j}^{(13)}z_{t-j}+\epsilon_{t}^{(1)} & \quad\mathrm{(a)}\\
y_{t}=\sum_{j=1}^{m}a_{j}^{(21)}x_{t-j}+\sum_{j=1}^{m}a_{j}^{(22)}y_{t-j}+\sum_{j=1}^{m}a_{j}^{(23)}z_{t-j}+\epsilon_{t}^{(2)} & \quad\mathrm{(b)}\\
z_{t}=\sum_{j=1}^{m}a_{j}^{(31)}x_{t-j}+\sum_{j=1}^{m}a_{j}^{(32)}y_{t-j}+\sum_{j=1}^{m}a_{j}^{(33)}z_{t-j}+\epsilon_{t}^{(3)} & \quad\mathrm{(c)}
\end{aligned}
\right.\label{eq: Eq 3-var}
\end{equation}

\end_inset

where variables 
\begin_inset Formula $x_{t}$
\end_inset

, 
\begin_inset Formula $y_{t}$
\end_inset

, 
\begin_inset Formula $z_{t}$
\end_inset

 can be column vectors, coefficient 
\begin_inset Formula $a^{(ij)}$
\end_inset

 can be matrix.
\end_layout

\begin_layout Standard
In order to solve Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var"

\end_inset

, left multiply 
\begin_inset Formula $x_{t-k}^{T}$
\end_inset

, 
\begin_inset Formula $y_{t-k}^{T}$
\end_inset

, 
\begin_inset Formula $z_{t-k}^{T}$
\end_inset

 to each of them, then take expectation.
 e.g.
 left multiply 
\begin_inset Formula $x_{t-k}^{T}$
\end_inset

 to (a) of Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var"

\end_inset

, then take expectation:
\begin_inset Formula 
\begin{equation}
\mathrm{E}(x_{t}x_{t-k}^{T})=\sum_{j=1}^{m}a_{j}^{(11)}\mathrm{E}(x_{t-j}x_{t-k}^{T})+\sum_{j=1}^{m}a_{j}^{(12)}\mathrm{E}(y_{t-j}x_{t-k}^{T})+\sum_{j=1}^{m}a_{j}^{(13)}\mathrm{E}(z_{t-j}x_{t-k}^{T})+\mathrm{E}(\epsilon_{t}^{(1)}x_{t-k}^{T})\label{eq: Eq 3-var eq11}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
For convenience, define 
\begin_inset Formula $x^{(1)}=x$
\end_inset

, 
\begin_inset Formula $x^{(2)}=y$
\end_inset

, 
\begin_inset Formula $x^{(3)}=z$
\end_inset

, and
\begin_inset Formula 
\[
R^{(uv)}=\left(b_{jk}\right)=\left(\mathrm{E}(x_{t-j}^{(u)}x_{t-k}^{(v)T})\right),\ (j,k=1\ldots m).
\]

\end_inset

Here 
\begin_inset Formula $b$
\end_inset

 is a placeholder variable to indicate row and column index.
 
\begin_inset Formula $u$
\end_inset

 and 
\begin_inset Formula $v$
\end_inset

 are indices of variables, it can be 
\begin_inset Formula $x$
\end_inset

, 
\begin_inset Formula $y$
\end_inset

 or 
\begin_inset Formula $z$
\end_inset

, or just 
\begin_inset Formula $1$
\end_inset

, 
\begin_inset Formula $2$
\end_inset

 or 
\begin_inset Formula $3$
\end_inset

.
 For example, 
\begin_inset Formula $R^{(xy)}=\left(b_{jk}\right)=\left(\mathrm{E}(x_{t-j}y_{t-k}^{T})\right)$
\end_inset

, its 
\begin_inset Formula $j$
\end_inset

-row, 
\begin_inset Formula $k$
\end_inset

-column element is 
\begin_inset Formula $\mathrm{E}(x_{t-j}y_{t-k}^{T})$
\end_inset

.
 
\begin_inset Formula $R^{(uv)T}=R^{(vu)}$
\end_inset

.
 Define
\begin_inset Formula 
\[
\vec{a}^{(uv)}=\left(b_{j}\right)=\left(a_{j}^{(uv)}\right),\ (j=1\ldots m).
\]

\end_inset


\end_layout

\begin_layout Standard
For example, 
\begin_inset Formula $\vec{a}^{(12)}=\left(b_{j}\right)^{T}=\left(a_{j}^{(12)}\right)^{T},\ (j=1\ldots m)$
\end_inset

, it's a row vector (also might be partitioned matrix).
 Define
\begin_inset Formula 
\[
\vec{v}^{(u|v)}=\left(b_{k}\right)^{T}=\left(\mathrm{E}(x_{t}^{(u)}x_{t-k}^{(v)T})\right)^{T},\ (k=1\ldots m).
\]

\end_inset


\end_layout

\begin_layout Standard
For example, 
\begin_inset Formula $\vec{v}^{(x|y)}=\left(b_{k}\right)^{T}=\left(\mathrm{E}(x_{t}y_{t-k}^{T})\right)^{T},\ (k=1\ldots m)$
\end_inset

.
 It's row vector.
\end_layout

\begin_layout Standard
By use above symbols, Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var"

\end_inset

(a) now satisfy
\begin_inset Formula 
\begin{equation}
\left\{ \begin{gathered}\vec{v}^{(x|x)}=\vec{a}^{(11)}R^{(xx)}+\vec{a}^{(12)}R^{(yx)}+\vec{a}^{(13)}R^{(zx)}\\
\vec{v}^{(x|y)}=\vec{a}^{(11)}R^{(xy)}+\vec{a}^{(12)}R^{(yy)}+\vec{a}^{(13)}R^{(zy)}\\
\vec{v}^{(x|z)}=\vec{a}^{(11)}R^{(xz)}+\vec{a}^{(12)}R^{(yz)}+\vec{a}^{(13)}R^{(zz)}
\end{gathered}
\right.\label{eq: Eq 3-var clear}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
note 
\begin_inset Formula $\mathrm{E}(\epsilon_{t}^{(u)}x_{t-k}^{(v)T})=0,\ \forall k=1\ldots m$
\end_inset

.
 And similarly for (b), (c) of Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var"

\end_inset

.
\end_layout

\begin_layout Standard
We need to solve 
\begin_inset Formula $\vec{a}^{(uv)}$
\end_inset

, 
\begin_inset Formula $\mathrm{var}(\epsilon_{t}^{(u)})\left(=\mathrm{E}(\epsilon_{t}^{(u)}x_{t}^{(u)T})\right)$
\end_inset

, then 2-var AR, then finally GC.
\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Standard
For simplicity, further assume 
\begin_inset Formula $x_{t}$
\end_inset

, 
\begin_inset Formula $y_{t}$
\end_inset

, 
\begin_inset Formula $z_{t}$
\end_inset

 are white noise each, hence 
\begin_inset Formula $R^{(uu)}=v^{(u)}I,\, v^{(u)}\triangleq\mathrm{var}(x_{t}^{(u)})$
\end_inset

 
\begin_inset Foot
status open

\begin_layout Plain Layout
\begin_inset Formula $I$
\end_inset

 denote the identical matrix.
 For vector 
\begin_inset Formula $x^{(u)}$
\end_inset

, if 
\begin_inset Formula $x^{(u)}$
\end_inset

 is not whitened element-wise, 
\begin_inset Formula $R^{(uu)}$
\end_inset

 is a blocked diagonal matrix.
\end_layout

\end_inset

, and 
\begin_inset Formula $\vec{v}^{(x|x)}=0$
\end_inset

.
\end_layout

\begin_layout Standard
Rewrite Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var clear"

\end_inset

 as:
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{ccc}
\vec{a}^{(11)} & \vec{a}^{(12)} & \vec{a}^{(13)}\end{array}\right]\left[\begin{array}{ccc}
v^{(x)}I & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & v^{(y)}I & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & v^{(z)}I
\end{array}\right]=\left[\begin{array}{ccc}
0 & \vec{v}^{(x|y)} & \vec{v}^{(x|z)}\end{array}\right].\label{eq: Eq 3-var more clear}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
In order to solve the variance of residual, in Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var eq11"

\end_inset

, let 
\begin_inset Formula $k=0$
\end_inset

, get
\begin_inset Formula 
\begin{equation}
\mathrm{var}(x_{t}^{(1)})=\vec{a}^{(12)}\vec{v}^{(x|y)T}+\vec{a}^{(13)}\vec{v}^{(x|z)T}+\mathrm{var}(\epsilon_{t}^{(1)}).
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Due to Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var more clear"

\end_inset

, it's
\begin_inset Formula 
\begin{equation}
\mathrm{var}(\epsilon_{t}^{(1)})=\mathrm{var}(x_{t}^{(1)})-\left[\begin{array}{ccc}
0 & \vec{v}^{(x|y)} & \vec{v}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
v^{(x)}I & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & v^{(y)}I & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & v^{(z)}I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{v}^{(x|y)T}\\
\vec{v}^{(x|z)T}
\end{array}\right].
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Or in correlation form, 
\begin_inset Formula $S^{(xy)}\triangleq R^{(uv)}/\sqrt{v^{(u)}v^{(v)}}$
\end_inset

, 
\begin_inset Formula $\vec{s}^{(u|v)}\triangleq\vec{v}^{(u|v)}/\sqrt{v^{(u)}v^{(v)}}$
\end_inset

 (row vector)
\begin_inset Foot
status open

\begin_layout Plain Layout
Here the definition of 
\begin_inset Formula $S^{(xy)}$
\end_inset

 and 
\begin_inset Formula $\vec{s}^{(u|v)}$
\end_inset

 are for scalar 
\begin_inset Formula $v^{(u)}$
\end_inset

.
 See section 
\begin_inset CommandInset ref
LatexCommand ref
reference "sub:Derivation-Detail-of"

\end_inset

 for derivation detail.
\end_layout

\end_inset

:
\begin_inset Formula 
\begin{equation}
\mathrm{var}(\epsilon_{t}^{(1)})=\mathrm{var}(x_{t}^{(1)})\left(I-\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
I & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & I & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right]\right).\label{eq: var(eps)<-S}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Note that in this expression, 
\begin_inset Formula $Z$
\end_inset

 need a full regression.
\end_layout

\begin_layout Standard
Through similar process, get 2-var AR solution without variable 
\begin_inset Formula $y$
\end_inset

:
\begin_inset Formula 
\begin{equation}
\mathrm{var}(\epsilon_{t}^{A(1)})=\mathrm{var}(x_{t}^{(1)})\left(I-\left[\begin{array}{cc}
0 & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{cc}
I & S^{(xz)}\\
S^{(zx)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|z)T}
\end{array}\right]\right)
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
For scalars 
\begin_inset Formula $x_{t}$
\end_inset

, 
\begin_inset Formula $y_{t}$
\end_inset

 the GC is
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}=\ln\Biggl(\frac{1-\left[\begin{array}{cc}
0 & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{cc}
I & S^{(xz)}\\
S^{(zx)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|z)T}
\end{array}\right]}{1-\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
I & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & I & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right]}\Biggr).\label{eq:GC3}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Approximation of Three-Variable GC
\end_layout

\begin_layout Subsubsection
Rude Approximation
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $\vec{s}^{(u|v)}$
\end_inset

 and 
\begin_inset Formula $S^{(uv)}$
\end_inset

 (
\begin_inset Formula $u\neq v$
\end_inset

) small, view the inverse matrix in Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:GC3"

\end_inset

 as identity matrix, get approximation
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}=\vec{s}^{(x|y)}\vec{s}^{(x|y)T}\approx\sum_{k=1}^{m}\vec{s}_{k}^{(x|y)}\vec{s}_{k}^{(x|y)}=\sum_{k=1}^{m}\frac{\mathrm{E}(x_{t}y_{t-k})^{2}}{\mathrm{var}(x_{t})\mathrm{var}(y_{t})}\label{eq: GC y->x approx}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Since 
\begin_inset Formula $z_{t}$
\end_inset

 in Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var"

\end_inset

 might be a vector, Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: GC y->x approx"

\end_inset

 is also the n-var approximate GC expression.
\end_layout

\begin_layout Standard
Similarly through Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var more clear"

\end_inset

, get coefficients (row (blocked) vector)
\begin_inset Formula 
\begin{equation}
\left\{ \begin{aligned}\vec{a}^{(11)}\approx & 0\\
\vec{a}^{(12)}\approx & \vec{v}^{(x|y)}\left(v^{(y)}\right)^{-1}=(b_{k})=\left(\mathrm{E}(x_{t}y_{t-k})/\mathrm{var}(y_{t})\right)\quad k=1,\ldots,m\\
\vec{a}^{(13)}\approx & \vec{v}^{(x|z)}\left(v^{(z)}\right)^{-1}=(b_{k})=\left(\mathrm{E}(x_{t}z_{t-k}^{T})/\mathrm{var}(z_{t})\right)
\end{aligned}
\right.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
This approximation essentially treat the joint regression pairwisely.
\end_layout

\begin_layout Subsubsection
Slightly Better Approximation
\end_layout

\begin_layout Standard
Now we try to correct the above result (Eq.
  
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: GC y->x approx"

\end_inset

) to reflect effect from other variables.
\end_layout

\begin_layout Standard
We have
\begin_inset Formula 
\[
\ln(\frac{1}{1-b})\approx b,\quad\textrm{for }b\ll1.
\]

\end_inset


\begin_inset Formula 
\[
b=1-\frac{1-d}{1-c}\approx d-c,\quad\textrm{for }d,c\ll1,
\]

\end_inset


\end_layout

\begin_layout Standard
So Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:GC3"

\end_inset

 become
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}=\ln\Biggl(\frac{1-c}{1-d}\Biggr)\approx d-c,\quad\textrm{for }d,c\ll1,\label{eq:d-c app GC(y->x)}
\end{equation}

\end_inset

where
\begin_inset Formula 
\begin{equation}
c=\left[\begin{array}{cc}
0 & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{cc}
I & S^{(xz)}\\
S^{(zx)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|z)T}
\end{array}\right]\label{eq:def c}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
d=\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
I & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & I & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & I
\end{array}\right]^{-1}\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right]\label{eq:def d}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Under the small 
\begin_inset Formula $S^{(uv)}$
\end_inset

 assumption,
\begin_inset Formula 
\begin{equation}
(I-B)^{-1}\approx I+B,\quad\textrm{for }\left\Vert B\right\Vert \ll1,\label{eq:linear operator app}
\end{equation}

\end_inset


\begin_inset Formula 
\[
B_{2}=-\left[\begin{array}{cc}
O & S^{(xz)}\\
S^{(zx)} & O
\end{array}\right],\quad B_{3}=-\left[\begin{array}{ccc}
O & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & O & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & O
\end{array}\right]
\]

\end_inset


\begin_inset Formula 
\[
c\approx\left[\begin{array}{cc}
0 & \vec{s}^{(x|z)}\end{array}\right]\left(I+B_{2}\right)\left[\begin{array}{c}
0\\
\vec{s}^{(x|z)T}
\end{array}\right]
\]

\end_inset


\begin_inset Formula 
\[
d\approx\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left(I+B_{3}\right)\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right].
\]

\end_inset


\end_layout

\begin_layout Standard
Get the corrected GC (compared with Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: GC y->x approx"

\end_inset

)
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}\approx\vec{s}^{(x|y)}\vec{s}^{(x|y)T}-2\vec{s}^{(x|y)}S^{(yz)}\vec{s}^{(x|z)T}.\label{eq:GCapp-od2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
In covariance form (Still assume 
\begin_inset Formula $x_{t}$
\end_inset

, 
\begin_inset Formula $y_{t}$
\end_inset

 are scalars) (See section 
\begin_inset CommandInset ref
LatexCommand eqref
reference "sub:Derivation-Detail-of"

\end_inset

 for definition of 
\begin_inset Formula $\Lambda^{(z)}$
\end_inset

)
\begin_inset Formula 
\[
F_{y\rightarrow x|z}\approx\frac{\vec{v}^{(x|y)}\vec{v}^{(x|y)T}-2\vec{v}^{(x|y)}R^{(yz)}\left(\Lambda^{(z)}\right)^{2}\vec{v}^{(x|z)T}}{\mathrm{var}(x_{t})\mathrm{var}(y_{t})}.
\]

\end_inset


\end_layout

\begin_layout Standard
For vector 
\begin_inset Formula $z$
\end_inset

, Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:GCapp-od2"

\end_inset

 can be writen in component form
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}\approx\vec{s}^{(x|y)}\vec{s}^{(x|y)T}-2\vec{s}^{(x|y)}\sum_{k=1}^{n_{z}}S^{(yz[k])}\vec{s}^{(x|z[k])T}.\label{eq:GCapp-od2-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}\approx\frac{1}{\mathrm{var}(x_{t})\mathrm{var}(y_{t})}\left(\vec{v}^{(x|y)}\vec{v}^{(x|y)T}-2\vec{v}^{(x|y)}\sum_{k=1}^{n_{z}}\frac{R^{(yz[k])}\vec{v}^{(x|z[k])T}}{\mathrm{var}(z[k]_{t})}\right).\label{eq:GCapp-od2-1-cov}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
But Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:GCapp-od2-1"

\end_inset

 is not suitable for large 
\begin_inset Formula $n$
\end_inset

 (say 
\begin_inset Formula $n>10$
\end_inset

), due to 
\begin_inset Formula $\left\Vert B_{2}\right\Vert \ll1$
\end_inset

 and 
\begin_inset Formula $\left\Vert B_{3}\right\Vert \ll1$
\end_inset

 not well satisfied any more (hence the approximation Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:linear operator app"

\end_inset

 is not good or even not valid).
\end_layout

\begin_layout Standard
Recall
\begin_inset Formula 
\[
F_{y\rightarrow x|z}\approx d-c=\vec{s}^{(x|y)}\vec{s}^{(x|y)T}+\left[\begin{array}{ccc}
0 & \vec{s}^{(x|y)} & \vec{s}^{(x|z)}\end{array}\right]\left(\left[\begin{array}{ccc}
I & S^{(xy)} & S^{(xz)}\\
S^{(yx)} & I & S^{(yz)}\\
S^{(zx)} & S^{(zy)} & I
\end{array}\right]^{-1}-\left[\begin{array}{ccc}
I & 0 & S^{(xz)}\\
0 & I & 0\\
S^{(zx)} & 0 & I
\end{array}\right]^{-1}\right)\left[\begin{array}{c}
0\\
\vec{s}^{(x|y)T}\\
\vec{s}^{(x|z)T}
\end{array}\right]
\]

\end_inset

where 
\begin_inset Formula $F_{y\rightarrow x}\approx\vec{s}^{(x|y)}\vec{s}^{(x|y)T}$
\end_inset

, so we get the relationship between conditional GC and pairwise GC.
\end_layout

\begin_layout Subsubsection
A Far Better Approximation
\end_layout

\begin_layout Standard
Start from Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:d-c app GC(y->x)"

\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:def c"

\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:def d"

\end_inset

 again.
\end_layout

\begin_layout Standard
In covariance form, define
\begin_inset Formula 
\[
\left[\begin{array}{ccc}
Q^{(xx)} & Q^{(xy)} & Q^{(xz)}\\
Q^{(yx)} & Q^{(yy)} & Q^{(yz)}\\
Q^{(zx)} & Q^{(zy)} & Q^{(zz)}
\end{array}\right]=\left[\begin{array}{ccc}
R^{(xx)} & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & R^{(yy)} & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & R^{(zz)}
\end{array}\right]^{-1},
\]

\end_inset


\begin_inset Formula 
\[
c^{(R)}=\left[\begin{array}{cc}
\vec{r}^{(x|x)} & \vec{r}^{(x|z)}\end{array}\right]\left[\begin{array}{cc}
R^{(xx)} & R^{(xz)}\\
R^{(zx)} & R^{(zz)}
\end{array}\right]^{-1}\left[\begin{array}{cc}
\vec{r}^{(x|x)} & \vec{r}^{(x|z)}\end{array}\right]^{T},
\]

\end_inset


\begin_inset Formula 
\[
d^{(R)}=\left[\begin{array}{ccc}
\vec{r}^{(x|x)} & \vec{r}^{(x|y)} & \vec{r}^{(x|z)}\end{array}\right]\left[\begin{array}{ccc}
R^{(xx)} & R^{(xy)} & R^{(xz)}\\
R^{(yx)} & R^{(yy)} & R^{(yz)}\\
R^{(zx)} & R^{(zy)} & R^{(zz)}
\end{array}\right]^{-1}\left[\begin{array}{ccc}
\vec{r}^{(x|x)} & \vec{r}^{(x|y)} & \vec{r}^{(x|z)}\end{array}\right]^{T}.
\]

\end_inset

Note that:
\begin_inset Formula 
\[
F_{y\rightarrow x|z}=-\ln\left(1-\frac{\frac{1}{\mathrm{var}(x_{t})}\left(d^{(R)}-c^{(R)}\right)}{1-\frac{1}{\mathrm{var}(x_{t})}c^{(R)}}\right),
\]

\end_inset

get
\begin_inset Formula 
\[
F_{y\rightarrow x|z}\approx\frac{1}{\mathrm{var}(x_{t})}\left(d^{(R)}-c^{(R)}\right)=\frac{1}{\mathrm{var}(x_{t})}\vec{a}^{(12)}\left(Q^{(yy)}\right)^{-1}\left(\vec{a}^{(12)}\right)^{T}.
\]

\end_inset


\end_layout

\begin_layout Standard
In correlation form (for the 
\begin_inset Quotes eld
\end_inset

z-score
\begin_inset Quotes erd
\end_inset

 data),
\begin_inset Formula 
\[
F_{y\rightarrow x|z}\approx d-c=\vec{a}^{(12)}\left(Q^{(yy)}\right)^{-1}\vec{a}^{(12)T}
\]

\end_inset


\end_layout

\begin_layout Standard
If data is further whitenned (as in Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq: Eq 3-var more clear"

\end_inset

), then
\begin_inset Formula 
\begin{eqnarray*}
\left(Q^{(yy)}\right)^{-1} & = & I-S^{(yz)}S^{(zy)}-\left(S^{(yx)}-S^{(yz)}S^{(zx)}\right)\left(I-S^{(xz)}S^{(zx)}\right)^{-1}\left(S^{(xy)}-S^{(xz)}S^{(zy)}\right)\\
 & \approx & I-S^{(yz)}S^{(zy)}-S^{(yx)}S^{(xy)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
For a very weak GC, we might even use
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x|z}\approx\vec{a}^{(12)}\vec{a}^{(12)T}\label{eq:GC coef app}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsubsection
Relation of Joint Regression and Pairwise Regression
\end_layout

\begin_layout Standard
If we do a 
\begin_inset Formula $x$
\end_inset

 regression with 
\begin_inset Formula $y$
\end_inset


\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
\vec{b}^{(11)} & \vec{b}^{(12)}\end{array}\right]\left[\begin{array}{cc}
R^{(xx)} & R^{(xy)}\\
R^{(yx)} & R^{(yy)}
\end{array}\right]=\left[\begin{array}{cc}
\vec{r}^{(x|x)} & \vec{r}^{(x|y)}\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Then from similar way to Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:GC coef app"

\end_inset

, we get
\begin_inset Formula 
\begin{equation}
F_{y\rightarrow x}\approx d_{xy}-c_{xy}=b^{(12)}\left(Q^{(yy)}-Q^{(yz)}\left(Q^{(zz)}\right)^{-1}Q^{(zy)}\right)^{-1}\left(b^{(12)}\right)^{T}\approx\vec{b}^{(12)}\vec{b}^{(12)T}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
So the relation between 
\begin_inset Formula $\vec{a}^{(uv)}$
\end_inset

 and 
\begin_inset Formula $\vec{b}^{(uv)}$
\end_inset

 is important for understanding the relation between joint regression and
 pairwise regression 
\begin_inset Formula 
\begin{equation}
b^{(12)}=a^{(12)}-a^{(13)}\left(Q^{(zz)}\right)^{-1}Q^{(zy)}\label{eq:b-from-a}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
a^{(12)}\left(I-\left(Q^{(yy)}\right)^{-1}Q^{(yz)}\left(Q^{(zz)}\right)^{-1}Q^{(zy)}\right)=b^{(12)}+b_{[xz]}^{(13)}\left(Q^{(zz)}\right)^{-1}Q^{(zy)}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
\vec{b}^{(11)} & \vec{b}^{(12)}\\
\vec{b}^{(21)} & \vec{b}^{(22)}
\end{array}\right]=\left[\begin{array}{cc}
\vec{a}^{(11)} & \vec{a}^{(12)}\\
\vec{a}^{(21)} & \vec{a}^{(22)}
\end{array}\right]+\left[\begin{array}{c}
a^{(13)}\\
a^{(23)}
\end{array}\right]\left[\begin{array}{cc}
R^{(zx)} & R^{(zy)}\end{array}\right]\left[\begin{array}{cc}
R^{(xx)} & R^{(xy)}\\
R^{(yx)} & R^{(yy)}
\end{array}\right]^{-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Implies: 
\begin_inset Formula $b^{(12)}=0,\ b^{(13)}=0\ \Rightarrow\ a^{(12)}=0$
\end_inset

, also 
\begin_inset Formula $a^{(13)}=0$
\end_inset

.
\end_layout

\begin_layout Standard
From Eq.
 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:b-from-a"

\end_inset

, and note that 
\begin_inset Formula $Q^{(uv)}$
\end_inset

 are approximately toeplitz matrix, in frequency domain (
\begin_inset Formula $B_{xy}\sim\F[b^{(12)}]$
\end_inset

 etc.).
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
B_{xy}\approx\left[A_{xy}-A_{xz}Q_{zz}^{-1}Q_{zy}\right]_{+}
\]

\end_inset


\end_layout

\begin_layout Standard
Under assumption
\begin_inset Formula 
\[
\Sigma=\mathrm{diag}\left(\left[\begin{array}{ccc}
\Sigma_{xx} & \Sigma_{yy} & \Sigma_{zz}\end{array}\right]\right)
\]

\end_inset


\begin_inset Formula 
\[
B_{xy}\approx\left[A_{xy}-A_{xz}\left(A_{zy}+\Sigma_{zz}A_{yz}^{*T}\Sigma_{yy}^{-1}\right)\right]_{+}
\]

\end_inset


\end_layout

\begin_layout Standard
For scaler 
\begin_inset Formula $x$
\end_inset

 and 
\begin_inset Formula $y$
\end_inset

 (
\begin_inset Formula $z$
\end_inset

 might be vector), and further assume 
\begin_inset Formula $\Sigma_{zz}\approx\Sigma_{yy}$
\end_inset

 (which is the usual case), we get
\begin_inset Formula 
\[
B_{xy}\approx\left[A_{xy}-A_{xz}\left(A_{zy}+A_{yz}^{*}\right)\right]_{+}
\]

\end_inset


\end_layout

